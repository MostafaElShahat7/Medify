{
  "info": {
    "name": "Medify API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {

      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/auth/register",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\",\n  \"role\": \"patient\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/auth/login",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          }
        }
      ]
    },
    {
      "name": "Doctor",
      "item": [
        {
          "name": "Create Profile",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/doctors/profile",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": {
                "specialization": "Cardiology",
                "experienceYears": 10,
                "availability": [
                  {
                    "day": "monday",
                    "startTime": "09:00",
                    "endTime": "17:00"
                  }
                ]
              }
            }
          }
        },
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/doctors/profile",
            "headers": {
              "Authorization": "Bearer {{token}}"
            }
          }
        },
        {
          "name": "Update Profile",
          "request": {
            "method": "PUT",
            "url": "{{baseUrl}}/api/doctors/profile",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": {
                "specialization": "Neurology",
                "experienceYears": 12
              }
            }
          }
        },
        {
          "name": "Update Availability",
          "request": {
            "method": "PUT",
            "url": "{{baseUrl}}/api/doctors/availability",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": {
                "availability": [
                  {
                    "day": "monday",
                    "startTime": "10:00",
                    "endTime": "18:00"
                  }
                ]
              }
            }
          }
        }
      ]
    },
    {
      "name": "Patient",
      "item": [
        {
          "name": "Create Profile",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/patients/profile",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": {
                "dateOfBirth": "1990-01-01",
                "gender": "male",
                "bloodType": "A_POSITIVE",
                "allergies": ["penicillin"],
                "emergencyContact": {
                  "name": "Jane Doe",
                  "relationship": "spouse",
                  "phone": "+1234567890"
                }
              }
            }
          }
        },
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/patients/profile",
            "headers": {
              "Authorization": "Bearer {{token}}"
            }
          }
        },
        {
          "name": "Add Medical History",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/patients/medical-history",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": {
                "condition": "Hypertension",
                "diagnosedDate": "2023-01-01",
                "medications": [
                  {
                    "name": "Lisinopril",
                    "dosage": "10mg",
                    "frequency": "daily"
                  }
                ]
              }
            }
          }
        }
      ]
    },
    {
      "name": "Appointments",
      "item": [
        {
          "name": "Create Appointment",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/appointments",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": {
                "doctorId": "{{doctorId}}",
                "appointmentDate": "2024-01-15T10:00:00Z",
                "reason": "Annual checkup"
              }
            }
          }
        },
        {
          "name": "Get Appointments",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/appointments",
            "headers": {
              "Authorization": "Bearer {{token}}"
            }
          }
        },
        {
          "name": "Update Appointment",
          "request": {
            "method": "PATCH",
            "url": "{{baseUrl}}/api/appointments/{{appointmentId}}",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": {
                "status": "completed",
                "notes": "Patient showed improvement"
              }
            }
          }
        }
      ]
    },
    {
      "name": "Medical Records",
      "item": [
        {
          "name": "Create Medical Report",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/medical-records/reports",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "diagnosis",
                  "value": "Common cold",
                  "type": "text"
                },
                {
                  "key": "symptoms",
                  "value": "[\"fever\", \"cough\"]",
                  "type": "text"
                },
                {
                  "key": "attachments",
                  "type": "file",
                  "src": []
                }
              ]
            }
          }
        },
        {
          "name": "Create Prescription",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/medical-records/prescriptions",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"medications\": [\n    {\n      \"name\": \"Paracetamol\",\n      \"dosage\": \"500mg\",\n      \"frequency\": \"Every 6 hours\",\n      \"duration\": \"5 days\"\n    }\n  ],\n  \"validUntil\": \"2024-01-25T00:00:00Z\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          }
        }
      ]
    },
    {
      "name": "Messages",
      "item": [
        {
          "name": "Send Message",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/messages",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"receiverId\": \"receiver_id\",\n  \"content\": \"Hello, how are you?\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          }
        },
        {
          "name": "Get Conversations",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/messages/conversations",
            "headers": {
              "Authorization": "Bearer {{token}}"
            }
          }
        }
      ]
    },
    {
      "name": "Search",
      "item": [
        {
          "name": "Search Doctors",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/search/doctors",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "params": {
              "specialization": "Cardiology",
              "rating": "4",
              "availability": "monday"
            }
          }
        },
        {
          "name": "Search Appointments",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/search/appointments",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "params": {
              "status": "scheduled",
              "startDate": "2024-01-01",
              "endDate": "2024-12-31"
            }
          }
        },
        {
          "name": "Search Medical Records",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/search/medical-records",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "params": {
              "diagnosis": "flu",
              "startDate": "2024-01-01",
              "endDate": "2024-12-31"
            }
          }
        }
      ]
    },
    {
      "name": "Reviews",
      "item": [
        {
          "name": "Create Review",
          "request": {
            "method": "POST",
            "url": "{{baseUrl}}/api/reviews/{{doctorId}}",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"rating\": 5,\n  \"comment\": \"Excellent doctor, very professional and caring.\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          }
        },
        {
          "name": "Get Doctor Reviews",
          "request": {
            "method": "GET",
            "url": "{{baseUrl}}/api/reviews/doctor/{{doctorId}}",
            "headers": {
              "Authorization": "Bearer {{token}}"
            }
          }
        },
        {
          "name": "Update Review",
          "request": {
            "method": "PUT",
            "url": "{{baseUrl}}/api/reviews/{{reviewId}}",
            "headers": {
              "Authorization": "Bearer {{token}}"
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"rating\": 4,\n  \"comment\": \"Updated review: Very good experience overall.\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          }
        },
        {
          "name": "Delete Review",
          "request": {
            "method": "DELETE",
            "url": "{{baseUrl}}/api/reviews/{{reviewId}}",
            "headers": {
              "Authorization": "Bearer {{token}}"
            }
          }
        }
      ]
    }
    
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000"
    },
    {
      "key": "token",
      "value": "your_jwt_token_here"
    }
  ]
  
}